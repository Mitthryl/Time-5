function adjustHeight(){if($(window).width()<='768'){jQuery('.heightmatch').height('auto');return;}var max_height=0;jQuery('.heightmatch').height('auto');jQuery('.heightmatch').each(function(){if(jQuery(this).outerHeight()>max_height){max_height=jQuery(this).outerHeight(true)-10;}});jQuery('.heightmatch').height(max_height);}jQuery(function(){jQuery('a.new-window').click(function(){window.open(this.href);return false;});adjustHeight();var resizeTimer;jQuery(window).resize(function(){clearTimeout(resizeTimer);resizeTimer=setTimeout(adjustHeight,250);});jQuery('#menu').superfish();});